	Gui.fmSettings.GsGcSettings.SetColumnProperty("gvSettings",V.Local.sFieldNames(V.Local.iCnt),V.Enum.ColumnPropertyNames!HeaderHAlignment,"Center")
F.Intrinsic.Control.Next(V.Local.iCnt)

